<html>
	<head>
		<title>All blogposts</title>
		<link rel='stylesheet' type='text/css' href='/static/style.css'>
		<meta charset='utf-8'/>
	</head>
	<body>
		<h1>Here are all posts</h1>
		{{range $post := .}}
			<p><a href="{{ $post.Link }}">{{ $post.Title }}</a> {{ $post.Date.Format "Jan 2, 2006" }} </p>
		{{end}}
	</body>
</html>
